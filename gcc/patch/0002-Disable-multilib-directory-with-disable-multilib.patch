From 4dcb87844ebd74e375ecc364cf85ec112d93a63a Mon Sep 17 00:00:00 2001
From: Michael Forney <mforney@mforney.org>
Date: Thu, 26 Mar 2020 17:08:26 -0700
Subject: [PATCH] Disable multilib directory with --disable-multilib

---
 gcc/Makefile.in | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/gcc/Makefile.in b/gcc/Makefile.in
index abae872cd63..9cb5f7991fe 100644
--- a/gcc/Makefile.in
+++ b/gcc/Makefile.in
@@ -2050,7 +2050,7 @@ libgcc.mvars: config.status Makefile specs xgcc$(exeext)
 multilib.h: s-mlib; @true
 s-mlib: $(srcdir)/genmultilib Makefile
 	if test @enable_multilib@ = yes \
-	   || test -n "$(MULTILIB_OSDIRNAMES)"; then \
+	   && test -n "$(MULTILIB_OSDIRNAMES)"; then \
 	  $(SHELL) $(srcdir)/genmultilib \
 	    "$(MULTILIB_OPTIONS)" \
 	    "$(MULTILIB_DIRNAMES)" \
